{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "zel",
      "camelcase": "Zel",
      "scope": "source.zel",
      "file-types": [
        "zel"
      ],
      "injection-regex": "^zel$",
      "class-name": "TreeSitterZel"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Zel grammar for tree-sitter",
    "authors": [
      {
        "name": "Brighton Cox",
        "email": "brightonlcox@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-zel"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}